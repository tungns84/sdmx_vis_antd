  "scripts": {
    "dev": "vite --open /ts-demo",
    "prepare": "husky install || true",
    "start:prod": "cross-env NODE_ENV=production DEBUG=webapp* node src/server",
    "start:dev": "cross-env NODE_ENV=development DEBUG=app* nodemon --watch src/server --exec 'node src/server'",
    "build": "vite build",
    "precommit": "lint-staged",
    "lint": "eslint src --color",
    "lint:eslint": "eslint",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:e2e:ui": "cross-env PROJECT=ui NODE_ENV=e2e yarn playwright test --ui --project=ui",
    "test:e2e:ci": "cross-env CI=true PROJECT=ui LOGGING_LEVEL=silent NODE_ENV=e2e yarn playwright test --project=ui",
    "test:e2e:a11y": "cross-env PROJECT=a11y LOGGING_LEVEL=silent NODE_ENV=e2e yarn playwright test --project=a11y",
    "i18n:extract": "formatjs extract \"src/web/**/!(*.test).js\" --out-file keys.json --id-interpolation-pattern [sha512:contenthash:base64:6]"
  },
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "yarn lint:eslint"
